{
  "extends": ["next/core-web-vitals", "plugin:import/errors", "plugin:import/warnings", "plugin:prettier/recommended"],
  "plugins": ["import", "prettier"],
  "rules": {
    "import/order": ["error", {
      "newlines-between": "never",
      "alphabetize": {
        "order": "asc",
        "caseInsensitive": true
      }
    }],
    "prettier/prettier": "error"
  }
}